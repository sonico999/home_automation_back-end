<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="CustomerBean" class="spring.test.Customer">
		<property name="person" ref="PersonBean"></property>

	</bean>

	<bean id="PersonBean" class="spring.test.Person">
		<property name="name" value="warren"></property>
		<property name="address" value="RozinjolFlt 9"></property>
		<property name="age" value="25"></property>
	</bean>
</beans>